{
    "name": "@jupyter_vre/experiment-manager",
    "version": "1.0.0",
    "description": "NaaVRE Experiment Manager",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/QCDIS/NaaVRE",
    "bugs": {
        "url": "https://github.com/QCDIS/NaaVRE/issues"
    },
    "license": "BSD-3-Clause",
