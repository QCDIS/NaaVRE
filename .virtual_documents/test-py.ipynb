import rpy2.robjects as ro

# Load the R script
with open('raw-r.r') as f:
    script = f.read()

# Evaluate the script in the R environment
ro.r(script)

# Get a list of all the variables
vars = ro.r("ls()")

# Convert the R vector to a Python list
vars = list(vars)

print(vars)
